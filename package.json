{
    "name": "playwright-structure-ts",
    "version": "1.0.6",
    "description": "Create a Playwright app for building env-ready",
    "bin": "bin/createPlaywrightStructureTS.js",
    "main": "server.js",
    "repository": "https://github.com/erenkorkmazemre/playwright-structure-ts",
    "author": "Eren Emre Korkmaz <erenkorkmazemre@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=12.0.0"
      },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "prettier": "npx prettier ./ --write",
        "start:test:ui": "npx prettier ./ --write & npx playwright test --ui",
        "start:test:ui:debug": "npx prettier ./ --write & npx playwright test --ui --debug",
        "start:test:login": "npx prettier ./ --write & npx playwright test login.spec.ts ",
        "start:test:login:ui": "npx prettier ./ --write & npx playwright test login.spec.ts --ui ",
        "start:test:login:debug": "npx prettier ./ --write & npx playwright test login.spec.ts --ui --debug",
        "start:test:local:integration": "npx prettier ./ --write & npx playwright test --grep '@integration'",
        "start:test:local:integration:ui": "npx prettier ./ --write & npx playwright test --grep '@integration' --ui",
        "start:test:local:integration:debug": "npx prettier ./ --write & npx playwright test --grep '@integration' --ui --debug",
        "start:test:dev:integration": "npx prettier ./ --write & npx playwright test --grep '@integration'",
        "start:test:stage:integration": "npx prettier ./ --write & npx playwright test --grep '@integration'",
        "start:test:local:e2e": "npx prettier ./ --write &  npx playwright test --grep '@e2e'",
        "start:test:local:e2e:ui": "npx prettier ./ --write & npx playwright test --grep '@e2e' --ui",
        "start:test:local:e2e:debug": "npx prettier ./ --write & npx playwright test --grep '@e2e' --ui --debug",
        "start:test:dev:e2e": "npx prettier ./ --write & npx playwright test --grep '@e2e'",
        "start:test:stage:e2e": "npx prettier ./ --write & npx playwright test --grep '@e2e'",
        "start:test:production:e2e": "npx prettier ./ --write & npx playwright test --grep '@e2e'",
        "start:test:branchbase:local": "npx prettier ./ --write & npx playwright test --grep $(grep FEATURE_BRANCH .env | cut -d '=' -f2)",
        "start:test:branchbase:dev": "npx prettier ./ --write & npx playwright test --grep $(grep FEATURE_BRANCH .env | cut -d '=' -f2)",
        "start:test:branchbase:stage": "npx prettier ./ --write & npx playwright test --grep $(grep FEATURE_BRANCH .env | cut -d '=' -f2)"
    },
    "keywords": [],
    "dependencies": {
        "@faker-js/faker": "^7.6.0",
        "@playwright/test": "^1.35.1",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "prettier": "2.8.8",
        "winston": "^3.8.2",
        "express": "^4.17.1",
        "mongoose": "^5.8.10",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@playwright/test": "^1.35.1",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "prettier": "2.8.8",
        "winston": "^3.8.2",
        "express": "^4.17.1",
        "mongoose": "^5.8.10",
        "cors": "^2.8.5"
    }
}
